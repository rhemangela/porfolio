    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.1",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.4",
    "element-ui": "^2.13.0",
    "jquery": "^3.5.0",
    "node-sass": "^4.13.0",
    "popper.js": "^1.16.1",
    "tailwindcss": "^1.2.0",
    "vee-validate": "^3.2.5",
    "vue": "^2.6.11",
    "vue-axios": "^2.1.5",
    "vue-cookies": "^1.6.1",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-router": "~4.2.0",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
